<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fise_dial_test_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.fisewatchlauncher2.FiseDialerTestActivity" >
    <EditText
        android:id="@+id/fise_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="10086"/>
    <Button
        android:id="@+id/fise_dial_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:background="@drawable/icon_fise_dial"
        android:text="">
    </Button>




</RelativeLayout>