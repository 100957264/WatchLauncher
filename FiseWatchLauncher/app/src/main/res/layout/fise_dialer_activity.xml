<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fise_second_activity"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:context="com.android.fisewatchlauncher.FiseLauncherActivity" >

    <Button
        android:id="@+id/app_dialer"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_centerInParent="true"
       android:onClick="dialButton"
        android:background="@drawable/icon_dialer" >
    </Button>

    <TextView
         android:id="@+id/ic_tv_dialer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/app_dialer"
        android:layout_centerHorizontal="true"
        android:text="@string/app_dialer"
        android:textSize="18sp" />


</RelativeLayout>