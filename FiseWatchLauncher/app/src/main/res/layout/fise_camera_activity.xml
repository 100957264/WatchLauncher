<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fise_six_activity"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.fisewatchlauncher.FiseLauncherActivity" >

    <Button
        android:id="@+id/app_camera"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:layout_centerInParent="true"
        android:background="@drawable/icon_camera" >
    </Button>

    <TextView
        android:id="@+id/ic_tv_camera"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/app_camera"
        android:layout_centerHorizontal="true"
        android:text="@string/app_camera"
        android:textSize="18sp" />


</RelativeLayout>